<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quickrepair">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".view.SearchTechnicians.SearchTechniciansActivity" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".view.User.LoginUser.LoginActivity" />
        <activity android:name=".view.Customer.RegisterCustomer.CustomerRegisterActivity" />

        <activity android:name=".view.Technician.RepairRequests.TechnicianRepairRequestsActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>